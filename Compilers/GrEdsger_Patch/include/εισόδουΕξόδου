κενό writeInteger(ακέραιος n);
κενό writeBoolean(λογικός b);
κενό writeChar(χαρακτήρας c);
κενό writeReal(δεκαδικός d);
κενό writeString(χαρακτήρας *s);
κενό readString(ακέραιος size, χαρακτήρας * s); 
ακέραιος readInteger();
λογικός readBoolean();
χαρακτήρας readChar();
δεκαδικός readReal();

κενό τύπωσεΑκέραιο(ακέραιος n) {
	writeInteger(n);
}
κενό τύπωσεΛογικό(λογικός n) {
	writeBoolean(n);
}
κενό τύπωσεΧαρακτήρα(χαρακτήρας n) {
	writeChar(n);
}
κενό τύπωσεΔεκαδικό(δεκαδικός n) {
	writeReal(n);
}
κενό τύπωσεΣυμβολοσειρά(χαρακτήρας *s) {
	writeString(s);
}
κενό διάβασεΣυμβολοσειρά(ακέραιος n,χαρακτήρας *s) {
	readString(n,s);
}
ακέραιος διάβασεΑκέραιο() { 
	επέστρεψε readInteger(); 
}
λογικός διάβασεΛογικό() { 
	επέστρεψε readBoolean(); 
}
χαρακτήρας διάβασεΧαρακτήρα() { 
	επέστρεψε readChar(); 
}
δεκαδικός διάβασεΔεκαδικό() { 
	επέστρεψε readReal(); 
}
