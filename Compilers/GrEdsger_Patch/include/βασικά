ακέραιος trunc (δεκαδικός d);
ακέραιος round (δεκαδικός d);
ακέραιος ord (δεκαδικός c);
χαρακτήρας chr (ακέραιος n);

ακέραιος ακΜέρος(δεκαδικός δ){
	επέστρεψε trunc(δ);
}

ακέραιος στρογγυλοποίηση(δεκαδικός δ){
	επέστρεψε round(δ);
}

ακέραιος οπα(δεκαδικός δ){
	επέστρεψε trunc(δ);
}

χαρακτήρας ακΣεΧαρ(ακέραιος δ){
	επέστρεψε chr(δ);
}
