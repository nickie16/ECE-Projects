BEGIN:	IN 10H
	
	MVI A,10H	;load blank space code
	STA 0B04H
	STA 0B05H
	
LOOPA:	LDA 2000H
	MOV B,A
	RLC		;check sign (MSB)
	JC MIN
PLS:	MVI A,10H
	STA 0B03H
	JMP POS
MIN:	MVI A,1CH
	STA 0B03H
	MOV A,B
	CMA
	INR A		;two's complement
	JMP KAT3

POS:	MOV A,B
KAT3:	MVI H,00H
	MVI L,00H
	MVI C,00H
LOOPB:	CPI 0AH
	JNC KAT
KAT2:	MOV L,A
	JMP OUTB
KAT:	INR H
	SUI 0AH
	JMP LOOPB
OUTB:	MOV A,H
	CPI 0AH
	JC GOON
	MVI C,01H
	MOV A,H
	SUI 0AH
	MOV H,A
GOON:	MOV A,C
	STA 0B02H
	MOV A,H
	STA 0B01H
	MOV A,L
	STA 0B00H
	LXI D,0B00H
	CALL STDM
	CALL DCD
	JMP LOOPA

	END